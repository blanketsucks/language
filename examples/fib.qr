import libc;

import std::range;
using Range from std::range;

func fib(n: i32) -> i32 {
    if (n == 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

func main() {
    for i in Range::new(0, 21) {
        libc::printf("%d\n", fib(i));
    }
}